@model IEnumerable<ProjetWeb.Models.Genre>

@{
    ViewBag.Title = "Oeuvre";
}

<h2>Artiste</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Libelle_Abrege)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Libelle_Abrege)
            </td>
            <td>
                @Html.ActionLink("Details", "DetailGenre", new { id = item.Code_Genre })
            </td>
        </tr>
    }

</table>
