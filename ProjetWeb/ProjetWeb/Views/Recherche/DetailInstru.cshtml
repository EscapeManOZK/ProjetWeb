@model IEnumerable<ProjetWeb.Models.Instrumentation>

@{
    ViewBag.Title = "Detail";
}

<h2>Artiste</h2>

    <table class="table">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Oeuvre.Titre_Oeuvre)
            </th>
            <th>
               Detail
            </th>
            <th></th>
        </tr>

        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Oeuvre.Titre_Oeuvre)
                </td>
                <td>
                   @Html.ActionLink("Details", "DetailOeuvre", new { id = item.Oeuvre.Code_Oeuvre })
                </td>
                <td></td>
            </tr>
        }

    </table>
