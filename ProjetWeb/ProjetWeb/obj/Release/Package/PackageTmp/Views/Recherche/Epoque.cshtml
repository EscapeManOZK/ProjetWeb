@model IEnumerable<ProjetWeb.Models.Oeuvre>
@{
    ViewBag.Title = "Epoque";
}

<h2>Epoque</h2>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Annee)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Titre_Oeuvre)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Annee)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Titre_Oeuvre)
            </td>
            <td>
                @Html.ActionLink("Details", "DetailOeuvre", new { id = item.Code_Oeuvre })
            </td>
        </tr>
    }

</table>
